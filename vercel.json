{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "/api/topHeadlines.js"
    },
    {
      "src": "^/keyword/(.*)",
      "dest": "/api/keywordSearch.js"
    }
  ]
}
